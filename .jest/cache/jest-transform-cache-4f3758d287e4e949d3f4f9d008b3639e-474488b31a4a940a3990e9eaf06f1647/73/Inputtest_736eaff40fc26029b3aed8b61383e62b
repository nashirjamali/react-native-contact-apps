var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _Input = _interopRequireDefault(require("../Input"));
var _jsxRuntime = require("react/jsx-runtime");
describe('Input', function () {
  test('renders correctly', function () {
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Input.default, {
        placeholder: "Enter your name"
      })),
      getByPlaceholderText = _render.getByPlaceholderText;
    var input = getByPlaceholderText('Enter your name');
    expect(input).toBeTruthy();
  });
  test('calls onChangeText when input text changes', function () {
    var onChangeTextMock = jest.fn();
    var _render2 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Input.default, {
        placeholder: "Enter your name",
        onChangeText: onChangeTextMock
      })),
      getByPlaceholderText = _render2.getByPlaceholderText;
    var input = getByPlaceholderText('Enter your name');
    _reactNative.fireEvent.changeText(input, 'John Doe');
    expect(onChangeTextMock).toHaveBeenCalledWith('John Doe');
  });
});
