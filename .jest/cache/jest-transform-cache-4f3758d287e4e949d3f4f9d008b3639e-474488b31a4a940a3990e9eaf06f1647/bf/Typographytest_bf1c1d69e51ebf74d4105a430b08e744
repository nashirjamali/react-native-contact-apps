var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _Typography = _interopRequireDefault(require("../Typography"));
var _jsxRuntime = require("react/jsx-runtime");
describe('Typography', function () {
  test('renders correctly with default props', function () {
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Typography.default, {
        children: "Hello, World!"
      })),
      getByText = _render.getByText;
    var textElement = getByText('Hello, World!');
    expect(textElement).toBeTruthy();
  });
  test('renders correctly with custom props', function () {
    var _render2 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Typography.default, {
        variant: "heading",
        color: "dark",
        align: "center",
        children: "Heading Text"
      })),
      getByText = _render2.getByText;
    var textElement = getByText('Heading Text');
    expect(textElement).toBeTruthy();
    expect(textElement.props.style).toEqual([{
      fontSize: 30,
      fontWeight: 'bold'
    }, {
      textAlign: 'center'
    }, {
      padding: 5
    }]);
  });
});
